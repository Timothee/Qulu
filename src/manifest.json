{
    "name": "Qulu â€“ Your Hulu Queue",
    "version": "0.4.4",
    "manifest_version": 2,

    "description": "Access your Hulu queue right from the toolbar! A badge shows you if you have anything to watch right now.",
    "icons": {
        "48": "images/logo_48x48.png"
    },

    "background": {
        "scripts": ["js/mixpanel.js", "js/background.js"]
    },
    "browser_action": {
        "default_title": "Qulu - Your Hulu Queue",
        "default_icon": {
            "19": "images/logo_gray_38x38.png",
            "38": "images/logo_gray_38x38.png"
        },
        "default_popup": "html/popup.html"
    },
    "permissions": [
        "*://*.hulu.com/*"
        ],
    "content_security_policy": "script-src 'self' https://cdn.mxpnl.com; object-src 'self'"
}
